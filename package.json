{
  "name": "craftcommand-root",
  "private": true,
  "version": "1.6.0",
  "author": "Extroos",
  "type": "module",
  "scripts": {
    "dev": "npm run dev --prefix frontend",
    "build": "npm run build --prefix frontend",
